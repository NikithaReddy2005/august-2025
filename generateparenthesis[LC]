n = int(input())
def generate(curr_str,ans,close,open,n):
    if(open+close == 2*n and open == close):
        ans.append(curr_str)
        return
    if(open>n):
        return
    if(close>open):
        return
    generate(curr_str+"(",ans,close,open+1,n)
    generate(curr_str+")",ans,close+1,open,n)
def generateparenthesis(n):
    curr_str=""
    ans=[]
    open=0
    close=0
    generate(curr_str,ans,close,open,n)
    return ans
print(generateparenthesis(n))
